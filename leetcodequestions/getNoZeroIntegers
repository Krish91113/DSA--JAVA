public class getNoZeroIntegers{
    
    public static int[] getNoZeroIntegers(int n) {
        for(int i=1;i<n;i++){
            int b = n-i;
            if(isValid(i) && isValid(b)){
                return new int[]{i,b};
            }
        }
        return new int[0];
    }
    private static boolean isValid(int num){
            while(num > 0){
                if(num % 10 ==0){
                    return false;
                }
                num = num/10;
            }
            return true;
        }
        public static void main (String args[]){
            int n=11;
            getNoZeroIntegers(n);
        }
}

